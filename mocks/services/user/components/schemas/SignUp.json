{
  "type": "object",
  "required": [
    "first_name",
    "last_name",
    "phone_number",
    "email",
    "password",
    "gender",
    "default_language",
    "accept_policy"
  ],
  "properties": {
    "first_name": {
      "type": "string",
      "maxLength": 70,
      "minLength": 2,
      "example": "John"
    },
    "last_name": {
      "type": "string",
      "maxLength": 70,
      "minLength": 2,
      "example": "Doeh"
    },
    "email": {
      "type": "string",
      "format": "email",
      "maxLength": 100,
      "example": "john.doeh@example.com"
    },
    "phone_number": {
      "type": "string",
      "example": "+3165432455"
    },
    "gender": {
      "type": "string",
      "example": "male",
      "enum": [
        "male",
        "female",
        "other",
        "unknown"
      ]
    },
    "password": {
      "type": "string",
      "format": "password",
      "example": "@#ThisIsStrong22Password!"
    },
    "default_language": {
      "type": "string",
      "maxLength": 2,
      "example": "EN"
    },
    "accept_policy": {
      "type": "boolean",
      "example": true
    }
  }
}

